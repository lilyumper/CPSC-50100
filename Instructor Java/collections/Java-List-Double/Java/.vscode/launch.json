  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Java: (External Terminal)",
      "request": "launch",
      "mainClass": "${workspaceFolder}/Start.java",
      "vmArgs": "--enable-preview",
      "console": "externalTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "java",
      "name": "Java: (Integrated Terminal)",
      "request": "launch",
      "mainClass": "${workspaceFolder}/Start.java",
      "vmArgs": "--enable-preview",
      "cwd": "${workspaceFolder}"
    }
  ]
}